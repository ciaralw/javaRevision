package javaRevision;

public class weekendVacationQ {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
			System.out.println(sleepIn);	
	}
	
	public boolean didWeSleepIn(boolean weekday, boolean vacation) {
		boolean sleepIn;
		if(!weekday || vacation) {
		sleepIn = true;
		}
		return sleepIn;
	}

}
